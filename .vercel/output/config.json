{
	"version": 3,
	"routes": [
		{
			"src": "^/_astro/(.*)$",
			"headers": {
				"cache-control": "public, max-age=31536000, immutable"
			},
			"continue": true
		},
		{
			"handle": "filesystem"
		},
		{
			"src": "^\\/_image$",
			"dest": "_render"
		},
		{
			"src": "^\\/Certificates\\/?$",
			"dest": "_render"
		},
		{
			"src": "^\\/CodigoAnteriorIndex\\/?$",
			"dest": "_render"
		},
		{
			"src": "^\\/Contact\\/?$",
			"dest": "_render"
		},
		{
			"src": "^\\/Experience\\/?$",
			"dest": "_render"
		},
		{
			"src": "^\\/Markdowm\\/Description\\/?$",
			"dest": "_render"
		},
		{
			"src": "^\\/Markdowm\\/Notas\\/?$",
			"dest": "_render"
		},
		{
			"src": "^\\/Posts\\/?$",
			"dest": "_render"
		},
		{
			"src": "^\\/Proyects\\/?$",
			"dest": "_render"
		},
		{
			"src": "^\\/Recursos\\/?$",
			"dest": "_render"
		},
		{
			"src": "^\\/Services\\/?$",
			"dest": "_render"
		},
		{
			"src": "^\\/Technologies\\/?$",
			"dest": "_render"
		},
		{
			"src": "^\\/$",
			"dest": "_render"
		}
	]
}